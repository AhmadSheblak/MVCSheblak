@model IEnumerable<DAL.User>

@{
    ViewBag.Title = "Index";
}


<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.User_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserRole.RoleName)
        </th>
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.User_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password, new { id = "password", Textmode = "Password" })
            </td>
            <td>

                @Html.DisplayFor(modelItem => item.RoleId)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.User_name }) |
                @Html.ActionLink("Details", "Details", new { id = item.User_name }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.User_name })
            </td>
        </tr>
    }

</table>
